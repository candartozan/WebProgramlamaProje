@model List<WebProgramlamaProje.Entities.Category>
<div class="d-flex justify-content-center">
    <div class="my-3 col-10">
        <h3 class="text-center">Kategoriler</h3>
        <ul class="list-group text-center">
            @foreach (var c in Model)
            {
                <li class="list-group-item list-group-item-action display-6">@c.Name</li>
            }
        </ul>
    </div>
</div>